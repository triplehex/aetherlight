{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true,
    "types": []
  },
  "include": ["src/**/*.ts", "js/**/*.ts"],
  "exclude": ["node_modules"]
}
